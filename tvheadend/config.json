{
    "name": "TVHeadend",
    "version": "0.1",
    "slug": "tvheadend",
    "description": "TVHeadend Add-On",
    "url": "https://github.com/tringler/hassio-tvheadend",
    "webui": "http://[HOST]:[PORT:9981]/",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "ports": {
      "9981/tcp": 9981,
      "9982/tcp": 9982

    },
        "ports_description": {
      "9981/tcp": "TVH Webinterface",
      "9982/tcp": "TVH HTSP"
    },
    "map": [
      "share:rw"
    ],
    "schema": {
    },
    "options": {
      "devices": ["/dev/ttyACM0:/dev/ttyACM0:rwm"]
  }
}